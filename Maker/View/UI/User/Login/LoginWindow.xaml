<Window x:Class="Maker.View.User.Login.LoginWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:transc="clr-namespace:Transitionals.Controls;assembly=Transitionals"
        mc:Ignorable="d"
        WindowStartupLocation="CenterScreen"
       Title="{DynamicResource LoginOrRegister}"  Height="500" Width="530">
    <Grid Background="#FF535353">
        <transc:TransitionElement x:Name="mainp" HorizontalAlignment="Center" VerticalAlignment="Center"/>
    </Grid>
</Window>
