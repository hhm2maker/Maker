<RibbonWindow x:Class="Maker.View.UI.Dialog.WindowDialog.ConfirmWindowDialog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Maker.View.UI.Dialog.WindowDialog"
        mc:Ignorable="d"
        WindowStyle="None" Height="300" Width="300" Background="{DynamicResource bgTitleColor}" WindowStartupLocation="CenterOwner" ShowInTaskbar="False">
    <WindowChrome.WindowChrome>
        <WindowChrome CaptionHeight="1" ResizeBorderThickness="2,0,2,2" CornerRadius="0" GlassFrameThickness="0" />
    </WindowChrome.WindowChrome>
    <Window.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="../../../StyleResources/ColorDictionary.xaml" />
                <ResourceDictionary Source="../../../StyleResources/AllDictionary.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Window.Resources>
    <Grid>
        <TextBlock Text="删除" Foreground="{DynamicResource TitleFontColor}"/>
    </Grid>
</RibbonWindow>
