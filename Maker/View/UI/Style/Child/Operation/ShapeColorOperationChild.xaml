<child:OperationStyle
    xmlns:child="clr-namespace:Maker.View.UI.Style.Child"
    x:Class="Maker.View.UI.Style.Child.ShapeColorOperationChild"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Maker.View.Dialog"
        xmlns:device="clr-namespace:Maker.View.Device"
        mc:Ignorable="d"
    x:Name="osContent"
       >
    <StackPanel Name="spContent" Orientation="Vertical">
        <DockPanel >
            <device:LaunchpadPro x:Name="mLaunchpad"/>
            <StackPanel VerticalAlignment="Center" Name="dpContent" Margin="20,0,0,0" />
        </DockPanel>
        <StackPanel VerticalAlignment="Center" Orientation="Horizontal" HorizontalAlignment="Center">
            <Button  Click="Preview" Foreground="White" FontSize="14" Width="80" Background="{x:Null}" BorderBrush="White" Margin="0,10,20,0"  Content="{DynamicResource Preview}"></Button>
            <Button  Click="PasteRangeListContent" Foreground="White" FontSize="14" Width="80" Background="{x:Null}" BorderBrush="White" Margin="0,10,20,0" Content="{DynamicResource PasteValue}" ></Button>
        </StackPanel>
    </StackPanel>
</child:OperationStyle>
