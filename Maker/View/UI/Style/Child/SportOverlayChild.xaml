<local:BaseChild x:Class="Maker.View.Style.Child.SportOverlayChild"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Maker.View.Style.Child"
             mc:Ignorable="d" 
            >
    <StackPanel Orientation="Vertical"   VerticalAlignment="Top" >
        <GroupBox  Margin="20,20,20,0" Header="{DynamicResource AccelerationOrDeceleration}" FontSize="14" Foreground="#F0F0F0" Style="{DynamicResource GroupBoxStyle1}">
            <StackPanel  Margin="5">
                <TextBox Background="{x:Null}" Name="tbSportOverlayNumber" Margin="0,0,0,0" Foreground="White" FontSize="14" ></TextBox>
            </StackPanel>
        </GroupBox>
    </StackPanel>
</local:BaseChild>
