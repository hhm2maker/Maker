<local:BaseChild x:Class="Maker.View.Style.Child.ColorOverlayChild"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Maker.View.Style.Child"
             mc:Ignorable="d" 
           >
    <StackPanel  Orientation="Vertical"  >
        <GroupBox  Margin="20,20,20,0" Header="{DynamicResource ColorSuperposition}" FontSize="14" Foreground="#F0F0F0" Style="{DynamicResource GroupBoxStyle1}">
            <StackPanel  Margin="5">
                <CheckBox  Foreground="#F0F0F0" FontSize="14" Name="cbFollow" Content="{DynamicResource  Follow}"></CheckBox>
                <TextBox Background="{x:Null}" Margin="0,10,0,0" Foreground="White" FontSize="14" Name="tbColorOverlayNumber"></TextBox>
            </StackPanel>
        </GroupBox>
    </StackPanel>
</local:BaseChild>
